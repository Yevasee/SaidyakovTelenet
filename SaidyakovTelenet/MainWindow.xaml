<Window x:Class="SaidyakovTelenet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaidyakovTelenet"
        mc:Ignorable="d"
        Title="Telenet" Height="450" Width="800" Icon="/images/Telenet_logo.png">
    <Grid HorizontalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Source="/images/Telenet_logo.png" HorizontalAlignment="Left" Height="60"/>
        <TextBlock Grid.Row="0" Text="Абоненты" HorizontalAlignment="Center" FontSize="24" VerticalAlignment="Center"></TextBlock>

        <Button Grid.Row="0" Margin="10" Padding="5" Name="BtnBack" Content="Назад" Click="BtnBack_Click" VerticalAlignment="Center" HorizontalAlignment="Right"/>

        <Frame Grid.Row="1" Name="FrameBase" NavigationUIVisibility="Hidden" ContentRendered="FrameBase_ContentRendered"></Frame>
    </Grid>
</Window>
